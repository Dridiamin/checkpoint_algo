ALGORITHM PROCEDURE dot_product
VAR
    v1,v2 : ARRAY_OF INTEGER[length];
    i,ps,n1,n2: INTEGER;
BEGIN

read (n1);
    FOR i FROM 0 TO n1 DO
        Read(V1[i])
    END_FOR

read(n2);
FOR i FROM 0 TO n2 DO
        Read(V2[i])
    END_FOR

dot_product(v1;v2,ps);

ps:= dot_product(v1,v2);
IF (ps=0) THEN
    
    write(v1 and v2 are orthogonal);
ELSE 
    write(v1 and v2 are not orthogonal);

 END_IF

END